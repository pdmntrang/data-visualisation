{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json", 
  "width": 500,
  "height": 400,
  "title": "Suicide rate in 2009 vs 2019",
  "data": {
      "url": "https://raw.githubusercontent.com/pdmntrang/data-visualisation/main/data/suicide-rate-wide.csv"
    },
  "transform": [
    {"filter": "datum.Y2019 > 0"},
    {"filter": "datum.Y2009 > 0"}
  ],
  "mark": "circle",
  "encoding": {
      "x": {
        "field": "Y2019",
        "type": "quantitative",
        "axis": {"tickCount": 15},
        "scale": {"type": "log", "domain": [1,150]}
       }, 
      "y": {
        "field": "Y2009",
        "type": "quantitative",
        "axis": {"tickCount": 15},
        "scale": {"type": "log", "domain": [1,150]}
      },
      "color": {
        "field": "Continent",
        "type": "nominal",
        "scale": {
          "domain": [
            "Africa",
            "Americas",
            "Eastern Mediterranean",
            "Europe",
            "South-East Asia",
            "Western Pacific"
          ],
          "range": [
            "#bbdefb",
            "#f4a261",
            "#f47068",
            "#cdb4db",
            "#e6ccb2",
            "#5390d9"
          ]
        }
      }
  }}